  object qJOB_P: TIBQuery
    Database = dmIS.dbIS
    Transaction = dmIS.mT
    OnCalcFields = qJOB_PCalcFields
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select k.id_team, k.id_p, k.amount, p.n_order, o.id_object, '
      'o.obj_name, s.name as oname, i.name as iname, f.t_personnel '
      
        'from (((((((projects p join product_objects o on o.id_order = p.' +
        'id_project)'
      'join product_versions v on v.id_version = o.id_version)'
      'join product_oper r on r.id_object = o.id_object)'
      
        'join product_traff f on f.id_prod_oper = r.id_prod_oper and f.id' +
        '_area = :pa1)'
      
        'join operations s on s.id_area = f.id_area and s.id_operation = ' +
        'f.id_operation)'
      'join product_plans l on l.id_prod_traf = f.id_prod_traff)'
      'left join items i on i.id_item = o.id_item)'
      'join product_plans_p k on k.id_prod_plan = l.id_prod_plan'
      'where v.status = 3 and l.d_out = :d1'
      'order by k.id_team, k.id_p, p.n_order')
    Left = 344
    Top = 136
    ParamData = <
      item
        DataType = ftInteger
        Name = 'pa1'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'd1'
        ParamType = ptUnknown
      end>
    object qJOB_PID_P: TIntegerField
      FieldName = 'ID_P'
      Origin = '"PRODUCT_PLANS_P"."ID_P"'
    end
    object qJOB_PID_TEAM: TIntegerField
      FieldName = 'ID_TEAM'
      Origin = '"PRODUCT_PLANS_P"."ID_TEAM"'
    end
    object qJOB_PAMOUNT: TFloatField
      FieldName = 'AMOUNT'
      Origin = '"PRODUCT_PLANS_P"."AMOUNT"'
    end
    object qJOB_PN_ORDER: TIBStringField
      FieldName = 'N_ORDER'
      Origin = '"PROJECTS"."N_ORDER"'
      Size = 16
    end
    object qJOB_PID_OBJECT: TIntegerField
      FieldName = 'ID_OBJECT'
      Origin = '"PRODUCT_OBJECTS"."ID_OBJECT"'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
    object qJOB_POBJ_NAME: TIBStringField
      FieldName = 'OBJ_NAME'
      Origin = '"PRODUCT_OBJECTS"."OBJ_NAME"'
      Size = 128
    end
    object qJOB_PONAME: TIBStringField
      FieldName = 'ONAME'
      Origin = '"OPERATIONS"."NAME"'
      Size = 30
    end
    object qJOB_PINAME: TIBStringField
      FieldName = 'INAME'
      Origin = '"ITEMS"."NAME"'
      Size = 128
    end
    object qJOB_PT_PERSONNEL: TFloatField
      FieldName = 'T_PERSONNEL'
      Origin = '"PRODUCT_TRAFF"."T_PERSONNEL"'
    end
    object qJOB_PnObj: TStringField
      FieldKind = fkCalculated
      FieldName = 'nObj'
      Size = 64
      Calculated = True
    end
    object qJOB_PfIO: TStringField
      FieldKind = fkLookup
      FieldName = 'fIO'
      LookupDataSet = dmIS.qPers
